<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">

    <xsd:element name="sudoku">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="cell" minOccurs="81" maxOccurs="81">
                    <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                            <xsd:pattern value="[1-9]?"></xsd:pattern>
                        </xsd:restriction>
                    </xsd:simpleType>
                </xsd:element>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>





</xsd:schema>